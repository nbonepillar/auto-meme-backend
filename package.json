{
  "name": "auto-meme-backend",
  "version": "1.0.0",
  "description": "A backend project built with NestJS and TypeScript.",
  "main": "dist/main.js",
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky",
    "check-lang-support": "node check-lang-support.cjs",
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@lifi/sdk": "^3.7.9",
    "@nestjs-modules/ioredis": "^2.0.2",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.2",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.2",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.2",
    "@nestjs/platform-socket.io": "^11.1.3",
    "@nestjs/platform-ws": "^11.1.3",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/websockets": "^11.1.3",
    "@solana/web3.js": "^1.98.2",
    "@swc/cli": "^0.7.7",
    "@swc/core": "^1.12.1",
    "@uniswap/sdk-core": "^4.0.7",
    "@uniswap/smart-order-router": "^3.15.0",
    "@uniswap/universal-router-sdk": "^1.2.2",
    "@uniswap/v2-sdk": "^3.2.0",
    "@uniswap/v3-sdk": "^3.10.0",
    "async-mutex": "^0.5.0",
    "async-retry": "^1.3.3",
    "axios": "^1.9.0",
    "bs58": "^6.0.0",
    "bunyan": "^1.8.15",
    "bunyan-blackhole": "^1.1.1",
    "cache-manager": "^7.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "ethers": "^5.8.0",
    "graphql-ws": "^6.0.5",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "nest-resend": "^3.1.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.0",
    "reflect-metadata": "^0.2.2",
    "resend": "^4.6.0",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.1",
    "swc-loader": "^0.2.6",
    "typeorm": "^0.3.24",
    "uuid": "^11.1.0",
    "viem": "^2.31.4",
    "winston": "^3.17.0",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@nestjs/testing": "^11.1.2",
    "@types/graphql": "^14.2.3",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.8",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-sonarjs": "^3.0.2",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "^src(.*)$": "<rootDir>/$1"
    },
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testTimeout": 20000
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
